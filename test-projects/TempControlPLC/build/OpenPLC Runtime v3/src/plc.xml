  <types>
    <dataTypes/>
    <pous>
      <pou name="main" pouType="program">
        <interface>
          <localVars>
            <variable name="Temp" address="%MW0">
              <type>
                <INT/>
              </type>
              <initialValue>
                <simpleValue value="260"/>
              </initialValue>
              <documentation>
                <xhtml:p><![CDATA[Simulated temperature in tenths of °C]]></xhtml:p>
              </documentation>
            </variable>
            <variable name="FanCmd" address="%QX0.0">
              <type>
                <BOOL/>
              </type>
              <initialValue>
                <simpleValue value="FALSE"/>
              </initialValue>
              <documentation>
                <xhtml:p><![CDATA[Cooling fan ON/OFF output]]></xhtml:p>
              </documentation>
            </variable>
            <variable name="HighT">
              <type>
                <INT/>
              </type>
              <initialValue>
                <simpleValue value="230"/>
              </initialValue>
              <documentation>
                <xhtml:p><![CDATA[High threshold = 23.0 °C]]></xhtml:p>
              </documentation>
            </variable>
            <variable name="LowT">
              <type>
                <INT/>
              </type>
              <initialValue>
                <simpleValue value="210"/>
              </initialValue>
              <documentation>
                <xhtml:p><![CDATA[Low threshold = 21.0 °C]]></xhtml:p>
              </documentation>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[(* If temp is high enough, turn fan ON *)
IF Temp >= HighT THEN
    FanCmd := TRUE;

(* If tempe is low enough, turn fan OFF *)
ELSIF Temp <= LowT THEN
    FanCmd := FALSE;
END_IF;]]></xhtml:p>
          </ST>
        </body>
        <documentation>
          <xhtml:p><![CDATA[ ]]></xhtml:p>
        </documentation>
      </pou>
